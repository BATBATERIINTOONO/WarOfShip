<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.overwatch.warofship">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".GameMenu.Menu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EndlessMode.EndlessModeActivity" />
        <activity android:name=".GameMenu.End" />
        <activity android:name=".StoryMode.Story1LevActivity" />
        <activity android:name=".StoryMode.Story2LevActivity" />
        <activity android:name=".StoryMode.Story3LevActivity" />
        <activity android:name=".StoryMode.Story4LevActivity" />
        <activity android:name=".StoryMode.Story5LevActivity" />
        <activity android:name=".GameMenu.StoryActivity"></activity>
    </application>

</manifest>